{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p -l 11ty:watch parcel:serve",
    "11ty:build": "npx @11ty/eleventy",
    "11ty:watch": "npx @11ty/eleventy --watch",
    "build": "run-s -l clean 11ty:build parcel:build",
    "clean": "rm -rf  _site/",
    "parcel:build": "parcel build \"./_site/**/*.html\" --dist-dir dist/",
    "parcel:serve": "parcel serve \"./_site/**/*.html\" --dist-dir dist/",
    "prestart": "run-s clean"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "@parcel/bundler-default": {
    "minBundles": 5
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@11ty/eleventy-navigation": "0.3.5",
    "@parcel/packager-raw-url": "2.7.0",
    "@parcel/transformer-webmanifest": "2.7.0",
    "@stefanracic/tailwind-child": "1.0.5",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/typography": "0.5.2",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "htmlhint": "1.1.4",
    "mdn-polyfills": "5.20.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.7.0",
    "postcss": "8.4.14",
    "postcss-mixins": "9.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "tailwindcss": "3.0.23"
  },
  "dependencies": {
    "algoliasearch": "4.13.1",
    "alpinejs": "3.10.2",
    "instantsearch.js": "4.43.0",
    "tippy.js": "6.3.7"
  }
}
