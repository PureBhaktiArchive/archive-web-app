{
  // Base config file used for VSCode ts language service
  // which provides refactoring and intellisense
  "compilerOptions": {
    "module": "Node16",
    "noImplicitReturns": true,
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "target": "ES2021"
  },
  "include": ["src"]
}
