{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --print-label 11ty:clean --parallel 11ty:watch parcel:serve",
    "build": "npm-run-all --print-label 11ty:clean 11ty:build parcel:build",
    "11ty:clean": "rm -rf _site/",
    "11ty:watch": "npx @11ty/eleventy --watch",
    "11ty:build": "npx @11ty/eleventy",
    "parcel:serve": "parcel serve \"./_site/**/*.html\" --dist-dir dist/",
    "parcel:build": "parcel build \"./_site/**/*.html\" --dist-dir dist/",
    "lint": "eslint .",
    "check-js-typing": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "@parcel/bundler-default": {
    "minBundles": 5
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.2",
    "@11ty/eleventy-navigation": "0.3.5",
    "@parcel/packager-raw-url": "2.8.2",
    "@parcel/transformer-webmanifest": "2.8.2",
    "@shopify/prettier-plugin-liquid": "1.0.2",
    "@stefanracic/tailwind-child": "1.0.5",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.7",
    "@types/alpinejs": "3.7.1",
    "dotenv": "16.0.3",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.26.0",
    "mdn-polyfills": "5.20.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.8.2",
    "postcss": "8.4.20",
    "postcss-mixins": "9.0.4",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "algoliasearch": "4.14.3",
    "alpinejs": "3.10.5",
    "instantsearch.js": "4.49.2",
    "tippy.js": "6.3.7"
  }
}
