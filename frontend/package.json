{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --print-label 11ty:clean parcel:clean --parallel 11ty:watch parcel:serve",
    "build": "npm-run-all --print-label 11ty:clean 11ty:build parcel:clean parcel:build",
    "11ty:clean": "rm -rf _site/",
    "11ty:watch": "npx @11ty/eleventy --watch --quiet",
    "11ty:build": "npx @11ty/eleventy --quiet",
    "parcel:clean": "rm -rf dist/",
    "parcel:serve": "parcel serve \"./_site/**/*.html\" --dist-dir dist/",
    "parcel:build": "parcel build \"./_site/**/*.html\" --dist-dir dist/",
    "lint": "eslint .",
    "check-js-typing": "tsc"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-navigation": "0.3.5",
    "@parcel/packager-raw-url": "2.8.3",
    "@parcel/transformer-webmanifest": "2.8.3",
    "@shopify/prettier-plugin-liquid": "1.1.0",
    "@stefanracic/tailwind-child": "1.0.5",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.7",
    "@types/alpinejs": "3.7.1",
    "@types/node": "18.16.2",
    "dotenv": "16.0.3",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.27.5",
    "mdn-polyfills": "5.20.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.8.3",
    "postcss": "8.4.20",
    "postcss-mixins": "9.0.4",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "tailwindcss": "3.3.2",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "algoliasearch": "4.14.3",
    "alpinejs": "3.12.1",
    "instantsearch.js": "4.49.2",
    "tippy.js": "6.3.7"
  }
}
