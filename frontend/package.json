{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "config": {
    "input": "src/",
    "intermediate": "tmp/",
    "output": "dist/"
  },
  "scripts": {
    "11ty:build": "eleventy --input=src/ --output=tmp/",
    "11ty:watch": "eleventy --input=src/ --output=tmp/ --watch",
    "build": "run-s -l clean 11ty:build parcel:build",
    "clean": "rimraf dist/ tmp/; mkdir -p dist/ tmp/",
    "dev": "run-p -l 11ty:watch parcel:serve",
    "parcel:build": "parcel build \"./tmp/**/*.html\" --dist-dir dist/",
    "parcel:serve": "parcel serve \"./tmp/**/*.html\" --dist-dir dist/",
    "predev": "run-s clean"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "@parcel/bundler-default": {
    "minBundles": 5
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@parcel/packager-raw-url": "2.7.0",
    "@parcel/transformer-webmanifest": "2.7.0",
    "@stefanracic/tailwind-child": "1.0.5",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/typography": "0.5.2",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.4",
    "htmlhint": "1.1.4",
    "mdn-polyfills": "5.20.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.7.0",
    "postcss": "8.4.14",
    "postcss-mixins": "9.0.3",
    "posthtml-expressions": "1.9.0",
    "posthtml-extend": "0.6.3",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.11",
    "rimraf": "3.0.2",
    "tailwindcss": "3.0.23"
  },
  "dependencies": {
    "algoliasearch": "4.13.1",
    "alpinejs": "3.10.2",
    "instantsearch.js": "4.43.0",
    "tippy.js": "6.3.7"
  }
}
