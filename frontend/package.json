{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development npm-run-all --print-label 11ty:clean --parallel 11ty:watch parcel:serve",
    "build": "cross-env NODE_ENV=production npm-run-all --print-label 11ty:clean 11ty:build parcel:build",
    "11ty:clean": "rm -rf _site/",
    "11ty:watch": "npx @11ty/eleventy --watch",
    "11ty:build": "npx @11ty/eleventy",
    "parcel:serve": "parcel serve \"./_site/**/*.html\" --dist-dir dist/",
    "parcel:build": "parcel build \"./_site/**/*.html\" --dist-dir dist/",
    "lint": "eslint ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "@parcel/bundler-default": {
    "minBundles": 5
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@11ty/eleventy-navigation": "0.3.5",
    "@parcel/packager-raw-url": "2.7.0",
    "@parcel/transformer-webmanifest": "2.7.0",
    "@shopify/prettier-plugin-liquid": "0.4.0",
    "@stefanracic/tailwind-child": "1.0.5",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/typography": "0.5.7",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.26.0",
    "mdn-polyfills": "5.20.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.7.0",
    "postcss": "8.4.17",
    "postcss-mixins": "9.0.4",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.2.0"
  },
  "dependencies": {
    "@11ty/eleventy-fetch": "3.0.0",
    "algoliasearch": "4.14.2",
    "alpinejs": "3.10.3",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "instantsearch.js": "4.46.3",
    "tippy.js": "6.3.7"
  }
}
