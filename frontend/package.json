{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel serve src/!(layout).html --port 3000 --dist-dir .serve",
    "build": "parcel build src/!(layout).html",
    "lint": "eslint ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "@parcel/bundler-default": {
    "minBundles": 5
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "2.2.1",
    "@parcel/transformer-webmanifest": "2.2.1",
    "@tailwindcss/aspect-ratio": "0.4.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-import": "2.25.3",
    "htmlhint": "1.1.2",
    "mdn-polyfills": "5.20.0",
    "parcel": "2.2.1",
    "postcss": "8.4.5",
    "postcss-mixins": "9.0.1",
    "postcss-preset-env": "7.2.3",
    "posthtml-expressions": "1.9.0",
    "posthtml-extend": "0.6.2",
    "prettier": "2.5.1",
    "prettier-plugin-tailwind": "2.2.12",
    "tailwindcss": "2.2.19",
    "tailwindcss-children": "2.1.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "0.4.1",
    "algoliasearch": "4.12.1",
    "alpinejs": "2.8.2",
    "instantsearch.js": "4.37.3",
    "tippy.js": "6.3.7"
  }
}
